#contact
	.container
		margin auto
		padding: 50px 0px 50px 18px
		max-width 1100px

.col-md-9.col-sm-8
	h3
		font-weight 400
.col-md-3.col-sm-4
	h3
		margin-top: 20px;
		margin-bottom: 10px;
		font-family: inherit;
		font-weight: 500;
		line-height: 1.1;
		color: #333
	ul.contact-right
		padding-left 0
		font-family: inherit;
		font-weight: 500;
		line-height: 1.1;
		color: #333
		li
			list-style: none;
			line-height: 30px;
			padding-bottom: 3px !Important;
			font-size: 16px;
			color #333
			i
				margin-right 10px
				font-size: 24px
			em
				font-style: normal
				font-weight: initial
				font-size: 13px
			.fa.fa-phone
				font-size: 18px
			.fa.fa-envelope-o
				font-size: 16px
			a
				color: #337ab7
				text-decoration: none
				font-weight: initial
	.social-right
		padding: 0px
		a
			background: #171717
			border-radius: 50%
			padding: 7px 13px 7px 13px
			color: #fff !important


