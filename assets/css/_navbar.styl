.navbar
	background: #eb4235
	margin-bottom 0px
	border-radius 0px
	li
		margin-left 25px
		a
			padding: 0
			line-height: 100px
			font-weight: bold !important
			background: transparent !important
			color: #ffcdd2!important
			font-weight: 500
			text-decoration: none !important
	:hover
		color #fff !important
.nav a.active
	color #fff !important
.navbar-toggle 
	margin-top 30px
	margin-right 0px
	.icon-bar
		background-color $white
.navbar-brand
	padding 0 15px 0 15px
	height 100px !important
	margin-left 0px !important
	img
		margin-top 20px
