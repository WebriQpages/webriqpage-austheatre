				.col-md-4.col-sm-6.col-xs-6
					.product-holder
						figure
							a(href='img/feature_products/Closed-Caption-Headset-receiver.jpg', data-lightbox='set')
								img(src='img/feature_products/Closed-Caption-Headset-receiver.jpg', class="img-responsive", alt='Closed Caption Headset Receiver')
