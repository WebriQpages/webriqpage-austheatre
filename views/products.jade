				.col-md-4.col-sm-6.col-xs-6
					.product-holder
						figure
							a(href='img/feature_products/USL-Hearing-Assistance.jpg', data-lightbox='set')
								img(src='img/feature_products/USL-Hearing-Assistance.jpg', class="img-responsive", alt='(CCS & HI/VI-N) Single Panel Two Channel')
						.product-title (CCS & HI/VI-N) Single Panel Two Channel
						.product-text
